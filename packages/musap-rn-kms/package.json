{
  "name": "@sphereon/ssi-sdk-ext.musap-rn-kms",
  "description": "Sphereon SSI-SDK react-native plugin for management of keys with musap.",
  "version": "0.22.0",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "your-library-name.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm && pnpm -g install @sphereon/ssi-sdk.dev || echo 'Skipping pnpm global install'",
    "prepare": "bob build",
    "release": "release-it",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Sphereon-OpenSource/SSI-SDK-crypto-extensions.git"
  },
  "author": "Sphereon <dev@sphereon.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Sphereon-OpenSource/SSI-SDK-crypto-extensions/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@veramo/core": "4.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.69.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.2.0",
    "prettier": "^2.4.1",
    "react": "18.2.0",
    "react-native": "0.71.0",
    "react-native-builder-bob": "^0.18.0",
    "release-it": "^14.11.0",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "../tsconfig.json"
        }
      ]
    ]
  }
}
